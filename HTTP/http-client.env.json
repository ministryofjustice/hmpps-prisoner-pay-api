{
  "local": {
    "baseUrl": "http://localhost:8080",
    "Security": {
      "Auth": {
        "orchestrator-auth-token": {
          "Type": "OAuth2",
          "Grant Type": "Client Credentials",
          "Client ID": "{{orchestrator-client-id}}",
          "Client Secret": "{{orchestrator-client-secret}}",
          "Token URL": "https://sign-in-dev.hmpps.service.justice.gov.uk/auth/oauth/token"
        },
        "ui-auth-token": {
          "Type": "OAuth2",
          "Grant Type": "Client Credentials",
          "Client ID": "{{ui-client-id}}",
          "Client Secret": "{{ui-client-secret}}",
          "Token URL": "https://sign-in-dev.hmpps.service.justice.gov.uk/auth/oauth/token"
        }
      }
    }
  },
  "dev": {
